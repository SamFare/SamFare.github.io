{
  "name": "samfare.io",
  "version": "1.0.0",
  "description": "Website detailing the carreer history of Sam Fare",
  "main": "index.js",
  "homepage": "https://samfare.github.io/website",
  "scripts": {
    "update": "jest --updateSnapshot",
    "start": "eslint src --fix && webpack-dev-server --hot --progress --colors",
    "build": "eslint src && webpack",
    "test": "eslint src --fix && jest",
    "test-update": "eslint src && jest --updateSnapshot",
    "testDebug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "deploy": "gh-pages -d build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Sam Fare",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "babel-loader": "^8.0.5",
    "babel-plugin-recharts": "^1.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-links": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "bootstrap": "^3.3.7",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-plugin-jest": "^21.13.0",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "gh-pages": "^1.1.0",
    "jest": "^24.0.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.2.0",
    "recharts": "^1.0.0-beta.10",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "webpack": "^4.8.3",
    "webpack-dev-server": "^3.1.11"
  },
  "dependencies": {
    "jest-each": "^24.0.0",
    "popper.js": "^1.13.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack-cli": "^3.2.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/mocks/styleMock.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>testSetup.js"
  }
}
